pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'BuildStarted'
        echo 'BuildFinished'
      }
    }

    stage('Test') {
      steps {
        echo 'testStarted'
      }
    }

    stage('deploy') {
      steps {
        echo 'deploymaster'
      }
    }

  }
}